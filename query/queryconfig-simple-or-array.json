{
  "http://oapass.org/ns/pass#Journal": {
    "or": [
      {
        "query": {
          "keys": [
            "nlmta"
          ],
          "q": "{{.Scheme}}://{{.HostAndPort}}/{{.Index}}?q={{$count := dec (len .KvPairs)}}{{range $i, $e := .KvPairs}}{{$e.Key}}:\"{{$e.Value}}\"{{if lt $i $count}}+{{end}}{{end}}&default_operator=AND"
        }
      },
      {
        "query": {
          "keys": [
            "journalName",
            "issn"
          ],
          "q": "{{.Scheme}}://{{.HostAndPort}}/{{.Index}}?q={{$count := dec (len .KvPairs)}}{{range $i, $e := .KvPairs}}{{$e.Key}}:\"{{$e.Value}}\"{{if lt $i $count}}+{{end}}{{end}}&default_operator=AND"
        }
      }
    ]
  }
}
